{
    "version": "2",
    "templates": [
        {
            "type": 1,
            "title": "Fire Dynamics Simulator (FDS) 6.7.6",
            "description": "Fire Dynamics Simulator (FDS) is a large-eddy simulation (LES) code for low-speed flows.",
            "categories": [
                "FDS"
            ],
            "platform": "linux",
            "logo": "https://www.nist.gov/sites/default/files/images/2022/03/09/Fire%20Safety%20Story.png",
            "image": "ap/fds-6.7.6:latest",
            "interactive": true,
            "env": [
                {
                    "name": "MPI",
                    "label": "Number of MPI Processes",
                    "default": "1"
                },
                {
                    "name": "OMP_NUM_THREADS",
                    "label": "Number of OpenMP Threads (OMP_NUM_THREADS)",
                    "default": "1"
                },
                {
                    "name": "FDS_FILE_PATH",
                    "label": ".fds exact file path",
                    "default": "THE_FDS_FILE_NAME.fds"
                }
            ],
            "volumes": [
                {
                    "container": "/workdir",
                    "bind": "/stockage/internal/YOUR_USERNAME/FDS_FOLDER",
                    "select_volume": true
                }
            ],
            "user": "YOUR_USERNAME_HERE",
            "network_mode": "none",
            "restart_policy": "unless-stopped",
            "entrypoint": "/entrypoint.sh"
        },
